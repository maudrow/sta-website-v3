{
  "name": "sta-website-v2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "DEBUG=true next dev",
    "build": "run-s build:next build:rss build:sitemap && next export",
    "build:next": "next build",
    "build:rss": "npm run script -- scripts/generate-rss.ts",
    "build:sitemap": "next-sitemap",
    "script": "ts-node --project tsconfig.scripts.json",
    "start": "next start",
    "prettier": "prettier --ignore-path .gitignore \"**/*.(js|jsx|json|yml|yaml|css|ts|tsx|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "next lint",
    "validate": "run-s check-format lint build"
  },
  "dependencies": {
    "@sta-podcast/post-loader": "^1.4.1",
    "bulma": "^0.9.4",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "next": "13.4.19",
    "node-fetch": "^3.3.2",
    "podcast": "^2.0.1",
    "proper-url-join": "^2.1.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-youtube": "^10.1.0",
    "sharp": "^0.32.6",
    "yaml": "^2.3.2"
  },
  "devDependencies": {
    "@sta-podcast/types": "^1.0.7",
    "@tsconfig/node16": "^16.1.1",
    "@types/node": "20.6.2",
    "@types/proper-url-join": "^2.1.1",
    "@types/react": "18.2.22",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "endent": "^2.1.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "next-sitemap": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.3",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  }
}
